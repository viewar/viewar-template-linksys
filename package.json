{
  "name": "viewar-boilerplate",
  "version": "0.0.1",
  "description": "boilerplate for viewar apps",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/matthiasklan/viewar-boilerplate.git"
  },
  "scripts": {
    "watch": "webpack --env production_debug --watch --config ./config/webpack.config.js & viewar-cli zip --watch",
    "deploy": "npm run bundle && npm run quick-deploy",
    "bundle": "npm run build",
    "build": "webpack --env production --config ./config/webpack.config.js && viewar-cli zip",
    "quick-deploy": "viewar-cli deploy",
    "deploy:debug": "npm run bundle:debug && npm run quick-deploy",
    "bundle:debug": "npm run build:debug && viewar-cli zip",
    "build:debug": "webpack --env production_debug --config ./config/webpack.config.js  && viewar-cli zip",
    "start": "webpack-dev-server --hot --env development --open --config ./config/webpack.config.js",
    "start:mock": "webpack-dev-server --hot --env development_mock  --open --config ./config/webpack.config.js",
    "start:remote": "npm run start:mock",
    "test": "babel-tape-runner \"./src/**/*.test.js\""
  },
  "author": "Matthias Klan for ViewAR GmbH",
  "license": "ISC",
  "dependencies": {
    "rxjs": "^5.5.2",
    "socket.io-client": "^2.0.4",
    "viewar-api": "^0.10.44",
    "viewar-call": "file:viewar-call",
    "viewar-core": "^11.0.0",
    "viewar-socket": "file:viewar-socket"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-imports": "^1.4.0",
    "babel-polyfill": "^6.8.0",
    "babel-preset-bluebird": "^1.0.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babili-webpack-plugin": "^0.1.2",
    "chokidar": "^1.6.0",
    "clean-webpack-plugin": "^0.1.16",
    "cross-env": "^5.0.1",
    "css-loader": "^0.23.1",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^3.0.0-beta.3",
    "fastclick": "^1.0.6",
    "file-loader": "^0.9.0",
    "git-revision-webpack-plugin": "^2.5.1",
    "glob": "^7.1.2",
    "happypack": "^4.0.0-beta.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "node-watch": "^0.5.5",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "postcss-apply": "^0.4.0",
    "postcss-cssnext": "^2.8.0",
    "postcss-loader": "^2.0.6",
    "postcss-responsive-type": "^0.4.0",
    "postcss-smart-import": "^0.7.4",
    "precss": "^2.0.0",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0"
  }
}
